{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "D:/zyc/zycTool/mingw64/mingw64/bin/g++.exe", // 这里写你自己的g++的路径
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "vector-of-bool.cmake-tools" // 这个很重要，就是这句话在通过cmake引入第三方库的代码补全
        }
    ],
    "version": 4
}
